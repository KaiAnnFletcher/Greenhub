{
    "env": {
        "es6": true,
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
      }
    },
    "plugins": [
        "react"
    ],
    "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
    ],
    "rules": {
        "react/no-set-state": "off"   
    }
}
