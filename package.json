{
  "name": "kaibru",
  "version": "1.0.0",
  "description": "sustainabililty application",
  "main": "server.ts",
  "scripts": {
    "test": "npm run lint && cross-env NODE_ENV=test mocha -u tdd --reporter spec --exit",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "lint": "eslint --quiet .",
    "fix": "eslint --fix .",
    "start:prod": "ts-node server.ts",
    "postinstall": "tsc",
    "watch-node": "nodemon server.ts",
    "watch-ts": "tsc -w",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "start:dev-server": "nodemon --ignore 'client/*' --exec babel-node server.ts",
    "scrape": "node routes/api/scrape.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KaiAnnFletcher/Kaibru.git"
  },
  "keywords": [
    "sustainability"
  ],
  "author": "Kai-Ann Fletcher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KaiAnnFletcher/Kaibru/issues"
  },
  "dependencies": {
    "@types/crypto-js": "^4.0.2",
    "@types/body-parser": "^1.19.1",
    "@types/passport": "^1.0.7",
    "@types/morgan": "^1.9.3",
    "amazon-pa-api50": "^0.2.0",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cheerio": "^1.0.0-rc.10",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.10",
    "mongoose": "^5.13.2",
    "morgan": "^1.10.0",
    "paapi5-nodejs-sdk": "^1.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "ts-node": "^10.2.0",
    "tslib": "^1.11.1",
    "typescript": "^4.5.4",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jquery": "^3.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.6.1",
    "@types/passport": "^1.0.7",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/request-promise": "^4.1.48",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "concurrently": "^6.4.0",
    "cross-env": "^5.2.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.4",
    "prettier": "1.19.1",
    "sinon": "1.17.3"
  }
}
